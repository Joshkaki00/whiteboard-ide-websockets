{
  "name": "leetcode-interview-assistant",
  "version": "1.0.0",
  "description": "Real-time collaborative coding interview practice tool",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsc -w && nodemon dist/server.js",
    "dev:client": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "vite build",
    "preview": "vite preview",
    "start": "node dist/server.js",
    "clean": "rm -rf dist && rm -rf dist-client"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "keywords": ["socket.io", "typescript", "interview", "coding", "collaborative"],
  "author": "Your Name",
  "license": "MIT"
}